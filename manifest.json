{
  "manifest_version": 2,
  "name": "SenecaAnswer",
  "version": "1.0",
  "author": "NavaShield",
  "description": "Gives seneca answers",
  "content_scripts": [
    {
      "matches": [
        "*://app.senecalearning.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "A tooltip popup description when you hover over the Chrome Extension icon."
  }
}
